PROGRAM
	
floatmaxmin
	
	//Global variables
      FLOAT Fnum1=350.56, Fnum2=780.0, Fnum3=930.4;
      FLOAT Fmax=0.0;
      FLOAT Fmin=0.0;
      
      PROC  minmax (FLOAT Fnum1, FLOAT Fnum2, FLOAT Fnum3, FLOAT Fmax, FLOAT Fmin)
        
        FLOAT Fmin = Fnum1;
        IF LF Fnum2  Fmin THEN
            Imin = Inum2;
        ENDIF
        IF LF Fnum3  Fmin THEN
            Fmin = Fnum3;
        ENDIF
        FLOAT Fmax = Fnum1;
        IF GF Fnum2  Fmax THEN
            Fmax = Fnum2;
ENDIF
        IF GF Fnum3   Fmax THEN
            Fmax = Fnum3;
     ENDIF
ENDPROC

MAIN
        PRINT (Fnum1);
        PRINT (Fnum2);
        PRINT (Fnum3);
        minmax();
        PRINT(Imin);
        PRINT(Imax);
ENDMAIN
	